{
  "name": "ng2-mock-server",
  "description": "Mock HTTP backend that runs in the browser to develop front-ends without back-ends.",
  "version": "0.0.4",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run compile-ts && npm run copy-ts && npm run copy-readme-license && npm run transform-package",
    "compile-ts": "ngc --project=tsconfig.aot.json",
    "clean": "rimraf dist/*",
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "transform-package": "node scripts/transformPackageJson.js",
    "copy-readme-license": "copyfiles README.md LICENSE dist",
    "copy-ts": "copyup \"src/**/*\" dist/ts"
  },
  "keywords": [
    "mock",
    "http",
    "server",
    "angular2",
    "angular4",
    "fake",
    "api"
  ],
  "author": "Vlad Velici (http://vladvelici.github.io)",
  "repository": "vladvelici/ng2-mock-server",
  "license": "MIT",
  "peerDependencies": {
    "@angular/core": "^2.3.1 || ^4.0.0",
    "@angular/http": "^2.3.1 || ^4.0.0",
    "rxjs": "^5.1.0"
  },
  "devDependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/compiler-cli": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@types/jasmine": "^2.5.46",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "copyfiles": "^1.0.0",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "jasmine": "^2.5.3",
    "karma": "^1.5.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.3",
    "multer": "^1.2.1",
    "phantomjs-prebuilt": "^2.1.14",
    "reflect-metadata": "^0.1.9",
    "rimraf": "^2.5.4",
    "rxjs": "^5.0.1",
    "typescript": "~2.4.1",
    "webpack": "^1.14.0",
    "zone.js": "^0.7.2"
  }
}
